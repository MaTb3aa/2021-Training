//==================================================================//
void run() {
ios::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);
#ifndef ONLINE_JUDGE
freopen("input.txt", "r", stdin);
#else
//	freopen("stations.in", "r", stdin);
#endif
}
//==================================================================//
const int N = 2000000+5, M = 18, mod = 998244353;

int phi[N],SODF[N];
bool prime[N];
int dp[N];
void solve(){
	prime[0]=prime[1]=1;
	for (int i = 0; i < N; ++i)phi[i]=1;
	for (int i = 2; i < N; ++i) {
		if(!prime[i]){
			phi[i]=i-1;
			for (int j = i+i; j < N; j+=i) {
				prime[j]=1;
				int pow=1,tmp=j;
				while(tmp%i==0){
					tmp/=i;
					pow*=i;
				}
				phi[j]*=(pow/i)*(i-1);
			}
		}

	}
}

int calc(int n){
	if(n==1)return 0;
	if(~dp[n])return dp[n];
	return dp[n]=calc(phi[n])+1;
}

int main() {
	run();
	solve();
	int t;
	cin>>t;
	clr(dp,-1);
	for (int i = 1; i < N; ++i) {
		SODF[i]=calc(i);
	}

	for (int i = 1; i < N; ++i) {
		SODF[i]+=SODF[i-1];
	}

	while(t--){
		int n,m;
		while(cin>>n>>m){
			cout<<SODF[m]-SODF[n-1]<<endl;
		}
	}


}

